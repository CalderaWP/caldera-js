{
  "name": "@calderajs/builder",
  "version": "0.9.13",
  "license": "GPL-2.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn test:watch",
    "test:watch": "jest --watch --silent",
    "test:ci": "jest --ci",
    "del-dist": "del-cli dist",
    "prebuild": "yarn del-dist",
    "build": "rm -rf dist && babel src -d dist --ignore \"src/**/*.fixture.js\",\"src/**/*.story.js\",\"src/**/*.test.js\"",
    "storybook": "start-storybook -p 6010",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix"
  },
  "peerDependencies": {
    "@wordpress/element": "^2.1.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@calderajs/components": "^0.9.12-alpha.3",
    "@storybook/addon-storysource": "^5.0.11",
    "@wordpress/block-editor": "^2.0.3",
    "@wordpress/components": "^7.4.0",
    "fuzzysearch": "^1.0.3",
    "keyboardjs": "^2.5.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "react-use": "^9.4.0",
    "rebound": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@calderajs/babel-preset-calderajs": "^0.9.10",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "glob-loader": "^0.3.0",
    "jest": "^23.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-svg-transformer": "^1.0.0",
    "nodemon": "^1.18.9",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2",
    "react-testing-library": "^6.1.2",
    "storybook-readme": "^5.0.3"
  },
  "jest": {
    "setupFiles": [
      "./config/jest/test-shim.js",
      "./config/jest/test-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "\\.(svg|css|scss|json)$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "\\.(svg|css|scss|json)$"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "<rootDir>/config/jest/nullMock.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b4736aef851415a133451016fdc583fba373c13d"
}
