{
  "name": "@calderajs/components",
  "version": "0.9.12-alpha.1",
  "license": "GPL-2.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn test:watch",
    "test:watch": "jest --watch --silent",
    "test:ci": "jest --ci",
    "del-dist": "del-cli dist",
    "prebuild": "yarn del-dist",
    "build": "rm -rf dist && babel src -d dist --ignore \"src/**/*.fixture.js\",\"src/**/*.story.js\",\"src/**/*.test.js\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@calderajs/babel-preset-calderajs": "^0.9.10",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "glob-loader": "^0.3.0",
    "jest": "^23.0",
    "jest-fetch-mock": "^2.1.0",
    "jest-svg-transformer": "^1.0.0",
    "nodemon": "^1.18.9",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2",
    "react-testing-library": "^6.1.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@rebass/grid": "^6.0.0",
    "@wordpress/compose": "^3.0.0",
    "@wordpress/element": "^2.1.8",
    "@wordpress/hooks": "^2.2.0",
    "accessible-autocomplete": "^1.6.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "deepmerge": "^3.0.0",
    "del-cli": "^1.1.0",
    "dot-prop": "^4.2.0",
    "email-validator": "^2.0.4",
    "formik": "^1.4.1",
    "lodash": "^4.17.11",
    "lodash.flatmap": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react-autocomplete": "^1.8.1",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-data-grid": "^5.0.5",
    "react-oembed-container": "^0.3.0",
    "react-quill": "^1.3.3",
    "react-transition-group": "^2.5.2",
    "shortid": "^2.2.14",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2",
    "yup": "^0.26.10"
  },
  "jest": {
    "setupFiles": [
      "./config/jest/test-shim.js",
      "./config/jest/test-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "\\.(svg|css|scss|json)$"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "\\.(svg|css|scss|json)$"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "<rootDir>/config/jest/nullMock.js"
    }
  },
  "gitHead": "fcdb234f24adb92bbb9620c7fd5f60493309b762",
  "publishConfig": {
    "access": "public"
  }
}
