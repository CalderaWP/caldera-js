{
  "name": "@calderawp/caldera-js",
  "workspaces": ["packages/*"],
  "scripts": {
    "start": "yarn --cwd packages/web-app dev",
    "build": "yarn --cwd packages/components build",
    "test:app": "yarn --cwd packages/caldera-forms-pro test",
    "test:components": "yarn --cwd packages/components test",
    "storybook:app": "yarn --cwd packages/caldera-forms-pro storybook",
    "storybook:components": "yarn --cwd packages/caldera-forms-pro storybook"
  }
}
